{
    "env": {
        "browser": true,
        "es2020": true,
        "jquery": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "no-lonely-if": "off",
        "no-underscore-dangle": "off"
    },
    "prettier/prettier": [
        "error",
        {
            "endOfLine": "auto"
        }
    ]
}
